<%- include('include/head.ejs') %>
<%- include('include/titleBar.ejs') %>
<%- include('include/sideNavBar.ejs') %>   
<div class="row"><!------------------Fila 1 ------------------------>
    <% for (programa of programas) { %>
        <div class="col s12 m6 l4"><!------------------Programa  ------------------------>
          <div class="card">
            <div class="card-image">
              <img src="https://www.fundacionsomos.cl/wp-content/uploads/2017/05/IMG_2792-960x750.jpeg">
            </div>
            <div class="card-content">
              <a href="/programas/programa1">
                <span class="card-title"><%= programa.nombre%></span>
              </a>
              <p>Ciclo: <%= programa.ciclo%></p>
              <p>Promedio: <%= programa.promedio%></p>
              <br>
              <div class="row">
                <div class="col s12">
                  <div class="nivel-programas">
                    <% for (nivel of programa.nivel) { %>
                      <!--<i class="material-icons red-text medium "><%= nivel%></i>
                      <i class="material-icons white-text medium"><%= nivel%></i>
                      -->
                      <a  href= '<%= programa.referencia %>' class="btn-large halfway-fab waves-effect waves-light red"><%= nivel%></a>
                    <% } %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div><!------------------Fin Programa  -------------------------------->
      <% } %>
    </div><!------------------Fin Programa Fila 1-------------------------------->

<%- include('include/footer.ejs') %>