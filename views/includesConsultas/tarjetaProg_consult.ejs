<% for (let i = 0; i< Math.ceil(numProg/4); i++) { %>
    <div class="carousel-item <%= color[i%(color.length)] %>" href="#<%= i+1 %>!">
        <div class="row">
            <div class="col s12"><br><br><br></div>
            <% for (let j = 0; j< 4 && (i*4+j < numProg); j++) { %>
            <div class="col s3">
                <div class="card blue-grey darken-1" style="padding: 0%; cursor:pointer;"id="<%= programasConsutas[i*4+j].idPrograma %>" onclick="selCard(event)">
                    <div class="card-content white-text">
                        <div class="row">
                            <div class="col s12">
                                <span class="card-title "><strong><%= programasConsutas[i*4+j].nombrePrograma %></strong></span>
                                
                            </div>
                            <div class="col s12">
                                <hr>
                                <br>
                                <p >Id Programa: &nbsp <%= programasConsutas[i*4+j].idPrograma %></p>
                                <br>
                                <p >Puntaje max: &nbsp <%= programasConsutas[i*4+j].puntajeMaximo %></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
<% } %>